@model PL.Models.ErrorViewModel
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>
</head>
<body>
    <hgroup>
        <h1>Error</h1>
        <h2>@Model.Message</h2>

    </hgroup>
    <div>
        @Html.ActionLink("Go back", Model.ViewName, Model.ControllerName,
            Model.RouteModel, htmlAttributes: new { @class = "btn btn-dark btn-lg" })
    </div>
</body>
</html>
